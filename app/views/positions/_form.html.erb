<%= form_for @position do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :status %>
  <%= f.text_field :status %>

  <%= f.fields_for :trade_details do |t| %>

    <%= t.label :direction %>
    <%= t.text_field :direction %><br />
    <%= t.label :size %>
    <%= t.text_field :size %><br />
    <%= t.label :product %>
    <%= t.text_field :product %><br />
    <%= t.label :trade_date %>
    <%= t.text_field :trade_date %><br />
    <%= t.label :price %>
    <%= t.text_field :price %><br />


  <% end %>
  <%= f.submit %>
<% end %>
