<div class="col-8">


  <%= form_for @user do |f| %>
    Email: <%= @user.email %><br />
    <br><%= f.fields_for :insights do |i| %>

      <%= i.label :title %>
      <%= i.text_field :title %><br />
      <%= i.label :content %>
      <%= i.text_area :content %><br />

    <% end %>
    <%= f.submit("Submit") %>
  <% end %>
</div>
